<p id="notice"><%= notice %></p>

<h1>Educational Resources</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @educational_resources.each do |educational_resource| %>
      <tr>
        <td><%= link_to 'Show', educational_resource %></td>
        <td><%= link_to 'Edit', edit_educational_resource_path(educational_resource) %></td>
        <td><%= link_to 'Destroy', educational_resource, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Educational Resource', new_educational_resource_path %>
